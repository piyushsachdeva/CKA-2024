  <script>
    (function () {
      const loginCardContainer = document.querySelector('main > section');
      if (
        window.location.pathname !== '/u/login/identifier' ||
        !loginCardContainer
      ) {
        return;
      }

      const content = document.querySelector('.content');
      if (content) {
        content.style.minHeight = '832px';
      }

      const signupCard = document
        .getElementById('signup-card')
        .content.cloneNode(true);

      signupCard.firstElementChild.setAttribute(
        'class',
        loginCardContainer.firstElementChild.getAttribute('class'),
      );

      loginCardContainer.appendChild(signupCard);
      const signupLink = signupCard.querySelector('.signup-link');
      signupLink.href += window.location.search;
    })();
  </script>
